{
    "dataset": {
        "path": "data\\",
        "data": "agricultural_product",
        "key": "date",
        "weekofday": "weekofday",

        "workers": 2,
        "stride": 1,

        "batch_size": 16,
        "seq_len": 18,
        "hidden_dim": 256,
        "train": {
            "opt": true,
            "train_rate": 0.8,
            "valid_rate": 0.1,
            "test_rate": 0.1
        }
    },
    "model": {
        "path": "models/pretrained/",
        "save": false,
        "load": true,
        "interval": 100,
        "tag": "10000"
    },    
    "train": {
        "epochs": {
            "base": 0,
            "iter": 1000,
            "critic": 5
        },
        "learning_rate": {
            "base": 0.0002,
            "gammaD": 1.0,
            "gammaG": 0.5
        },
        "bander":{
            "pivot": 6,
            "gp_weight": 1,
            "l1_gamma": 1,
            "l2_gamma": 1,
            "sigmas": {
                "inner": 0.5,
                "normal": 1.0,
                "warning": 2.0
            }
        }
    },
    "print": {
        "verbose": 1,
        "newline": 100
    },
    "visual": true
}